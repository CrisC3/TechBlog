
{{#if userBlog.length}}
<section>
  <h2>Your Posts</h2>
    {{#each userBlog as |post|}}
      {{> dashboard-postinfo post}}
    {{/each}}

</section>
{{/if}}

<button type="submit" class="btn">
  <a class="link" href="/dashboard/add">+ New Post</a>
</button>

